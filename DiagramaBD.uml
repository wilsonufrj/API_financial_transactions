@startuml
!theme mars

entity Wallet{
  *Long id
  --
  *Double allMoney
  *List<Gain> gains
  *List<Spend> spends
}

entity Gain{
  *Long id <PK>
  *Wallet wallet <FK>
--
  *Double Value
  *String day
  *String description
  *Boolean itMonthly
}

entity Spend{
  *Long id <PK>
  *Wallet wallet <FK>
--
  *Double Value
  *String day
  *String description
  *Boolean itMonthly
}

Wallet ||--o{Gain
Wallet ||--o{Spend

@enduml